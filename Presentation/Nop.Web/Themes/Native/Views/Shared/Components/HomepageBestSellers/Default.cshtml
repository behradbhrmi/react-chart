@model IList<ProductOverviewModel>
@if (Model.Count > 0)
{
    <div class="home-page-listbox bestsellers">
        <div class="title">
            <strong>@T("Bestsellers")</strong>
        </div>
        @foreach (var item in Model)
        {
            @await Html.PartialAsync("_ProductBox", item)
        }
    </div>
}